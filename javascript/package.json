{
  "name": "@jewelmusic/sdk",
  "version": "2.5.0",
  "description": "Official JewelMusic SDK for JavaScript/TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src tests --ext .ts,.js",
    "lint:fix": "eslint src tests --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js}\" \"examples/**/*.{js,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"tests/**/*.{ts,js}\" \"examples/**/*.{js,md}\"",
    "docs": "typedoc src/index.ts --out docs",
    "examples": "node examples/basic-usage.js",
    "examples:ai": "node examples/ai-generation.js",
    "examples:webhooks": "node examples/webhook-handling.js",
    "examples:workflow": "node examples/complete-workflow.js",
    "prepublishOnly": "npm run build && npm run test && npm run lint"
  },
  "keywords": [
    "jewelmusic",
    "music",
    "ai",
    "distribution",
    "transcription",
    "lyrics",
    "audio",
    "streaming",
    "api",
    "sdk"
  ],
  "author": "JewelMusic Team <developers@jewelmusic.art>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jewelmusic/sdk.git",
    "directory": "javascript"
  },
  "bugs": {
    "url": "https://github.com/jewelmusic/sdk/issues"
  },
  "homepage": "https://docs.jewelmusic.art/sdks/nodejs",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "form-data": "^4.0.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ]
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}