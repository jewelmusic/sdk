{
    "name": "jewelmusic/sdk",
    "description": "Official PHP SDK for JewelMusic AI-powered music distribution platform",
    "type": "library",
    "license": "MIT",
    "keywords": ["music", "ai", "distribution", "streaming", "analytics", "transcription"],
    "homepage": "https://jewelmusic.art",
    "support": {
        "docs": "https://docs.jewelmusic.art/sdk/php",
        "email": "support@jewelmusic.art",
        "issues": "https://github.com/jewelmusic/sdk/issues",
        "source": "https://github.com/jewelmusic/sdk/tree/main/php"
    },
    "authors": [
        {
            "name": "JewelMusic Team",
            "email": "developers@jewelmusic.art",
            "homepage": "https://jewelmusic.art"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/http-message": "^1.0|^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.7",
        "php-cs-fixer/shim": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "JewelMusic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JewelMusic\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "analyze": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}